<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.plus.mapper.order.CheckOrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mybatis.plus.entity.order.CheckOrder">
        <id column="check_order_id" property="checkOrderId" />
    <result column="create_time" property="createTime" />
    <result column="update_time" property="updateTime" />
        <result column="device_id" property="deviceId" />
        <result column="device_name" property="deviceName" />
        <result column="sth_id" property="sthId" />
        <result column="sth_name" property="sthName" />
        <result column="order_no" property="orderNo" />
        <result column="creator" property="creator" />
        <result column="status" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        create_time,
        update_time,
        check_order_id, device_id, device_name, sth_id, sth_name, order_no, creator, status
    </sql>

    <select id="invokeSql" resultType="java.util.HashMap">
       ${sqlString} limit 1000
    </select>

</mapper>
